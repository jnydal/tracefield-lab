{
  "uid": "pipeline-observability",
  "title": "Pipeline Observability",
  "tags": ["tracefield", "pipeline"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-7d",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Datasets",
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT datasets_total AS value FROM pipeline_counts;"
        }
      ],
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Entities",
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT entities_total AS value FROM pipeline_counts;"
        }
      ],
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Features",
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT features_total AS value FROM pipeline_counts;"
        }
      ],
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Analysis Jobs",
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT analysis_jobs_total AS value FROM pipeline_counts;"
        }
      ],
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Analysis Results",
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT analysis_results_total AS value FROM pipeline_counts;"
        }
      ],
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Stage Throughput (Daily)",
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT day AS \"time\", events, stage AS metric FROM pipeline_stage_daily WHERE status = 'ok' ORDER BY 1;"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 }
    },
    {
      "id": 7,
      "type": "table",
      "title": "Stale Datasets (SLA 24h)",
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT dataset_id, last_event_at, hours_since_last_event FROM pipeline_stuck ORDER BY hours_since_last_event DESC LIMIT 200;"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 }
    }
  ]
}
