{
  "uid": "pipeline-observability",
  "title": "Pipeline Observability",
  "tags": ["astro-reason", "pipeline"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-7d",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "People Processed",
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT people_total AS value FROM pipeline_counts;"
        }
      ],
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Traits Scored",
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT traits_scored AS value FROM pipeline_counts;"
        }
      ],
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Embeddings Computed",
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT embeddings_computed AS value FROM pipeline_counts;"
        }
      ],
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Astro Features",
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT astro_features_computed AS value FROM pipeline_counts;"
        }
      ],
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Correlations Computed",
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT correlations_computed AS value FROM pipeline_counts;"
        }
      ],
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Stage Throughput (Daily)",
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT day AS \"time\", events, stage AS metric FROM pipeline_stage_daily WHERE status = 'ok' ORDER BY 1;"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 }
    },
    {
      "id": 7,
      "type": "table",
      "title": "Stuck Jobs (SLA 24h)",
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT person_id, last_event_at, hours_since_last_event, missing_traits, missing_embeddings, missing_astro FROM pipeline_stuck ORDER BY hours_since_last_event DESC LIMIT 200;"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 }
    }
  ]
}
